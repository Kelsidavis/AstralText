{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Cargo Build",
            "type": "shell",
            "command": "cargo build",
            "problemMatcher": ["$rustc"],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "Cargo Test",
            "type": "shell",
            "command": "cargo test",
            "problemMatcher": ["$rustc"],
            "group": {
                "kind": "test",
                "isDefault": true
            }
        }
    ]
}
